{
  "restartable": "rs",
  "ignore": [
    ".git",
    "out/*",
    "node_modules/**/node_modules",
    "*.md",
    "doc/*",
    "logs/*"
  ],
  "verbose": false,
  "execMap": {
    "js": "electron ./dist/index.js"
  },
  "ext": "js html css",
  "watch": [
    "./src/index.js",
    "./src/renderer/*",
    "./src/public/*",
    "./src/model/*",
    "./src/model/service"
  ],
  "delay": "1000"
}
